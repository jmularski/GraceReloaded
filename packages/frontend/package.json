{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "6.1.2",
    "@material-ui/core": "4.11.4",
    "@material-ui/icons": "4.11.2",
    "@testing-library/jest-dom": "5.11.4",
    "@testing-library/react": "11.1.0",
    "@testing-library/user-event": "12.1.10",
    "@types/jest": "26.0.15",
    "@types/node": "12.0.0",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "cors": "2.8.5",
    "craco-alias": "3.0.1",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-scripts": "4.0.3",
    "socket.io-client": "4.1.2",
    "styled-components": "5.3.0",
    "typescript": "4.1.2",
    "web-vitals": "1.0.1"
  },
  "scripts": {
    "start": "./node_modules/.bin/craco start",
    "build": "./node_modules/.bin/craco build",
    "test": "./node_modules/.bin/craco test",
    "eject": "react-scripts eject",
    "test:e2e": "cd ./tests/e2e && cypress run",
    "lint": "eslint . -c ../../.eslintrc.json --ignore-path ../../.eslintignore --ext ts --ext tsx --ext js --ext jsx",
    "lint:fix": "eslint . --fix -c ../../.eslintrc.json --ignore-path ../../.eslintignore --ext ts --ext tsx --ext js --ext jsx"
  },
  "lint-staged": {
    "src/**/*.{ts, tsx}": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lerna run --concurrency 1 --stream precommit --since HEAD --exclude-dependents"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/node": "15.6.1",
    "@types/styled-components": "5.1.9",
    "@typescript-eslint/eslint-plugin": "4.25.0",
    "@typescript-eslint/parser": "4.25.0",
    "cypress": "7.4.0",
    "eslint": "7.27.0",
    "eslint-config-eslint": "7.0.0",
    "eslint-config-google": "0.14.0",
    "eslint-plugin-react": "7.23.2",
    "lint-staged": "11.0.0"
  }
}
